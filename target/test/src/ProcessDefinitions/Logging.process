<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pfx="http://www.tibco.com/schemas/Logging/Schemas/Schema.xsd" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:pfx2="http://www.tibco.com/schemas/maven-jenkins/Schema/Schema.xsd" xmlns:ns="http://www.tibco.com/pe/WriteToLogActivitySchema" xmlns:tib="http://www.tibco.com/bw/xslt/custom-functions">
    <xs:import xmlns:xs="http://www.w3.org/2001/XMLSchema" namespace="http://www.tibco.com/schemas/maven-jenkins/Schema/Schema.xsd" schemaLocation="/Schema/SchemaLog.xsd"/>
    <pd:name>ProcessDefinitions/Logging.process</pd:name>
    <pd:startName>Start</pd:startName>
    <pd:startType ref="pfx2:LogInfo"/>
    <pd:startX>82</pd:startX>
    <pd:startY>62</pd:startY>
    <pd:returnBindings/>
    <pd:endName>End</pd:endName>
    <pd:endX>462</pd:endX>
    <pd:endY>67</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1618909203869</pd:targetNamespace>
    <pd:activity name="LoggingDataInsert">
        <pd:type>com.tibco.plugin.jdbc.JDBCUpdateActivity</pd:type>
        <pd:resourceType>ae.activities.JDBCUpdateActivity</pd:resourceType>
        <pd:x>334</pd:x>
        <pd:y>66</pd:y>
        <config>
            <timeout>10</timeout>
            <commit>false</commit>
            <emptyStrAsNil>false</emptyStrAsNil>
            <jdbcSharedConfig>/Connections/JDBC_Connection.sharedjdbc</jdbcSharedConfig>
            <statement>insert into commonlog (id_log,idtech,idfonc,status,logmessage,date_time) values(?,?,?,?,?,?)</statement>
            <Prepared_Param_DataType>
                <parameter>
                    <parameterName>id_log</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>idtech</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>idfonc</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>status</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>logmessage</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
                <parameter>
                    <parameterName>date_time</parameterName>
                    <dataType>VARCHAR</dataType>
                </parameter>
            </Prepared_Param_DataType>
        </config>
        <pd:inputBindings>
            <jdbcUpdateActivityInput>
                <id_log>
                    <xsl:value-of select="tib:timestamp()"/>
                </id_log>
                <idtech>
                    <xsl:value-of select="$Start/pfx2:LogInfo/pfx2:idfonc"/>
                </idtech>
                <idfonc>
                    <xsl:value-of select="$Start/pfx2:LogInfo/pfx2:idtech"/>
                </idfonc>
                <status>
                    <xsl:value-of select="$Start/pfx2:LogInfo/pfx2:status"/>
                </status>
                <logmessage>
                    <xsl:value-of select="$Start/pfx2:LogInfo/pfx2:logmessage"/>
                </logmessage>
                <date_time>
                    <xsl:value-of select="current-dateTime()"/>
                </date_time>
            </jdbcUpdateActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="Log">
        <pd:type>com.tibco.pe.core.WriteToLogActivity</pd:type>
        <pd:resourceType>ae.activities.log</pd:resourceType>
        <pd:x>219</pd:x>
        <pd:y>68</pd:y>
        <config>
            <role>User</role>
        </config>
        <pd:inputBindings>
            <ns:ActivityInput>
                <message>
                    <xsl:value-of select="$Start/pfx2:LogInfo/pfx2:logmessage"/>
                </message>
                <msgCode>
                    <xsl:value-of select="$Start/pfx2:LogInfo/pfx2:status"/>
                </msgCode>
            </ns:ActivityInput>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>Start</pd:from>
        <pd:to>Log</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>LoggingDataInsert</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>Log</pd:from>
        <pd:to>LoggingDataInsert</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>